    "url": "https://github.com/Quid2/ts/issues"
  },
  "homepage": "https://github.com/Quid2/ts#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^7.0.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "es6-promise": "^4.2.8",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "tap": "^14.10.7",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.7",
    "typescript": "^3.8.0"
  },
  "dependencies": {
    "@quid2/ts-core": "git+https://github.com/Quid2/ts-core.git",
    "chai-as-promised": "^7.1.1",
    "doctest": "^0.17.1",
    "js-sha3": "^0.8.0",
    "queueing-subject": "^0.3.4",
    "rxjs": "^6.5.5",
    "ws": "^7.3.0"
  }
