  },
  "homepage": "https://github.com/Quid2/ts#readme",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "standard-version": "^4.3.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "doctest": "^0.13.0",
    "queueing-subject": "^0.2.0",
    "rxjs": "^5.5.6",
