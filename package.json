    "@types/chai": "^4.1.4",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^2.2.48",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "es6-promise": "^4.2.4",
    "mocha": "^5.2.0",
    "nyc": "^11.9.0",
    "standard-version": "^4.4.0",
    "tap": "^11.1.5",
    "ts-node": "^5.0.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "chai-as-promised": "^7.1.1",
    "doctest": "^0.13.0",
    "js-sha3": "^0.7.0",
    "queueing-subject": "^0.2.0",
    "rxjs": "^5.5.11",
    "ws": "^4.1.0"
